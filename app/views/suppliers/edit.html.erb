<%= form_for @supplier do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
  <div>
    <%= f.label :phone %>
    <%= f.text_field :phone %>
  </div>
  <div>
    <%= f.label :shipping_cost %>
    <%= f.text_field :shipping_cost %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>